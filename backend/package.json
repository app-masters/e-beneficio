{
  "name": "backend",
  "version": "0.0.2",
  "description": "",
  "main": "src/index.ts",
  "engines": {
    "node": "13.x"
  },
  "scripts": {
    "start": "nodemon --config nodemon.json -- exec src/index.js",
    "start-production": "pm2-runtime src/index.js --max_restarts=3",
    "build": "tsc --build",
    "test": "node node_modules/.bin/jest --config ./jest.config.js -i",
    "create": "sequelize db:create",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo",
    "migrate:create": "sequelize migration:create",
    "migrate:fresh": "ts-node-dev --transpileOnly --project tsconfig.json database/recreate.ts && npm run migrate",
    "migrate:fresh-production": "node database/recreate.js && npm run migrate",
    "seed": "ts-node-dev --transpileOnly --project tsconfig.json database/seeders/index.ts",
    "seed-production": "node database/seeders/index.js",
    "checks": "npm run check-prettier && npm run check-lint && npm run check-typescript",
    "check-prettier": "prettier-check \"src/**/*.{js,ts,tsx}\" && prettier-check \"database/**/*.{js,ts,tsx}\"",
    "check-lint": "eslint \"src/**/*.{js,ts,tsx}\" --max-warnings 0 && eslint \"database/**/*.{js,ts,tsx}\" --max-warnings 0",
    "check-typescript": "tsc --noEmit",
    "cronjob-metrics": "ts-node-dev --transpileOnly --project tsconfig.json src/utils/cronjob.ts",
    "setup": "npm run migrate && npm run seed",
    "scrape": "ts-node-dev src/scripts/runScraping.js",
    "validate": "ts-node-dev src/scripts/runValidation.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/storage": "^4.7.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "3.5.30",
    "@types/body-parser": "1.19.0",
    "@types/cors": "^2.8.6",
    "@types/crypto-js": "^3.1.46",
    "@types/express": "4.17.6",
    "@types/express-fileupload": "^1.1.3",
    "@types/faker": "^4.1.12",
    "@types/jest": "^25.2.1",
    "@types/js-levenshtein": "^1.1.0",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/node": "^13.9.8",
    "@types/passport": "^1.0.3",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/sequelize": "^4.28.8",
    "@types/string-similarity": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "2.28.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "csv-writer": "^1.6.0",
    "csvtojson": "^2.0.10",
    "dataloader": "^2.0.0",
    "dotenv": "8.2.0",
    "eslint-plugin-jsdoc": "^23.0.0",
    "express": "4.17.1",
    "express-fileupload": "^1.1.7-alpha.3",
    "faker": "^4.1.0",
    "hashids": "^2.2.1",
    "js-levenshtein": "^1.1.6",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "node-cron": "^2.0.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.0.0",
    "pm2": "^4.2.3",
    "require-all": "3.0.0",
    "rollbar": "2.15.1",
    "sequelize": "^5.21.5",
    "sequelize-cli": "5.5.1",
    "string-similarity": "^4.0.1",
    "ts-node-dev": "1.0.0-pre.44"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.18",
    "@types/node-cron": "^2.0.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-prettier": "3.1.3",
    "husky": "^4.2.3",
    "jest": "^25.2.4",
    "nodemon": "^2.0.2",
    "prettier": "2.0.4",
    "prettier-check": "^2.0.0",
    "ts-jest": "^25.3.0",
    "typescript": "3.8.3"
  }
}
